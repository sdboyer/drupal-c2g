/**
 * Implementation of hook_install().
 */
function upload_install() {
  // Create tables.
  drupal_install_schema('upload');
}

/**
 * Implementation of hook_uninstall().
 */
function upload_uninstall() {
  // Remove tables.
  drupal_uninstall_schema('upload');
}
